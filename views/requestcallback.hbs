<main>
    <div class="container-fluid">
        <h1 class="mt-4">Request Callback</h1>
     
        <div class="card mb-4">
            
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>Email</th>
                                <th>Phone</th>
                                <th>Message</th>
                                <th>Posted On</th>
                                
                            </tr>
                        </thead>
                        
                        <tbody>
                              {{#each allrequest}}
                                  <tr>
                                    <td>{{this.fname}}</td>
                                    <td>{{this.femail}}</td>
                                    <td>{{this.fphone}}</td>
                                    <td>{{this.fmsg}}</td>
                                    <td>{{this.createdon}}</td>
                                   
                                  </tr>
                              {{/each}}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</main>

